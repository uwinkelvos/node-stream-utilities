{
	"compilerOptions": {
		"strict": true,
		"target": "es2018",
		"lib": ["es2018"],
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"declaration": true,
		"sourceMap": true,
		"listFiles": false,
		"esModuleInterop": true,
		"types": []
	}
}
